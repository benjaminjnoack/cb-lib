{
  "name": "cb-lib",
  "version": "0.1.0",
  "author": "Ben Noack",
  "description": "Coinbase Library",
  "license": "Apache-2.0",
  "repository": "https://github.com/benjaminjnoack/cb-lib",
  "type": "module",
  "bin": {
    "cb": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "main": "./dist/cb.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist",
    "dev": "tsx src/cli.ts",
    "lint": "eslint src test eslint.config.mjs vitest.config.ts",
    "lint:fix": "eslint src test eslint.config.mjs vitest.config.ts --fix",
    "pack:dry": "npm pack --dry-run",
    "prepare": "husky",
    "prepack": "npm run clean && npm run build",
    "release:check": "npm run lint && npm run typecheck && npm run test && npm run pack:dry",
    "smoke:bin": "node dist/cli.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc -p tsconfig.json"
  },
  "keywords": [
    "cli",
    "typescript"
  ],
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.3",
    "@types/readline-sync": "^1.4.8",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,mjs,cjs}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "axios": "^1.13.5",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.3.1",
    "env-paths": "^4.0.0",
    "jsonwebtoken": "^9.0.3",
    "readline-sync": "^1.4.10",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  }
}
